<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
       xmlns:app="http://schemas.android.com/apk/res-auto"
       tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout">

    <androidx.cardview.widget.CardView
            android:layout_width="0dp"
            android:onClick="onChangeGuide"
            android:id="@+id/view_start_top"
            android:background="@android:color/white"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/guide_h"
            app:layout_constraintEnd_toStartOf="@id/guide_top_v"
            android:contentDescription="start_top"
            android:layout_margin="4dp"
            android:layout_height="0dp"
            app:cardCornerRadius="8dp">

        <androidx.constraintlayout.widget.ConstraintLayout
                android:orientation="vertical"
                android:id="@+id/scene_root_constraint_start_top"
                android:padding="16dp"
                android:layout_width="match_parent"
                android:background="@android:color/white"
                android:layout_height="match_parent">

            <ImageView android:layout_width="20dp"
                       app:layout_constraintEnd_toEndOf="parent"
                       app:layout_constraintTop_toTopOf="parent"
                       android:layout_height="20dp"
                       android:id="@+id/img_icon"
                       android:src="@mipmap/ic_launcher_round"
            />


            <TextView android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:text="1 GB"
                      android:id="@+id/tv_size"
                      android:textSize="28sp"
                      android:textStyle="bold"
                      app:layout_constraintBottom_toTopOf="@id/tv_start_top_price"
                      app:layout_constraintStart_toStartOf="parent"
            />

            <TextView android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:id="@+id/tv_start_top_price"
                      android:visibility="gone"
                      app:layout_constraintBottom_toTopOf="@id/progress_bar_start_top"
                      app:layout_constraintStart_toStartOf="parent"
                      android:text="$1.49 / month"
                      android:textSize="16sp"
                      android:textColor="#708090"
            />
            <ProgressBar android:layout_width="0dp"
                         android:layout_height="5dp"
                         android:id="@+id/progress_bar_start_top"
                         app:layout_constraintStart_toStartOf="parent"
                         app:layout_constraintEnd_toEndOf="parent"
                         app:layout_constraintBottom_toBottomOf="parent"
                         android:progress="100"
                         style="@style/Base.Widget.AppCompat.ProgressBar.Horizontal"
            />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
            android:layout_width="0dp"
            android:contentDescription="end_top"
            android:onClick="onChangeGuide"
            android:id="@+id/view_end_top"
            android:background="@android:color/white"
            app:layout_constraintStart_toEndOf="@id/guide_top_v"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/guide_h"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_margin="4dp"
            android:layout_height="0dp"
            app:cardCornerRadius="8dp">

        <androidx.constraintlayout.widget.ConstraintLayout
                android:orientation="vertical"
                android:id="@+id/scene_root_constraint_end_top"
                android:padding="16dp"
                android:layout_width="match_parent"
                android:background="@android:color/white"
                android:layout_height="match_parent">

            <ImageView android:layout_width="20dp"
                       app:layout_constraintEnd_toEndOf="parent"
                       app:layout_constraintTop_toTopOf="parent"
                       android:layout_height="20dp"
                       android:id="@+id/img_icon_end_top"
                       android:src="@mipmap/ic_launcher_round"
            />


            <TextView android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:text="1 GB"
                      android:id="@+id/tv_size_end_top"
                      android:textSize="28sp"
                      android:textStyle="bold"
                      app:layout_constraintBottom_toTopOf="@id/tv_end_top_price"
                      app:layout_constraintStart_toStartOf="parent"
            />

            <TextView android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:id="@+id/tv_end_top_price"
                      android:visibility="gone"
                      app:layout_constraintBottom_toTopOf="@id/progress_bar_end_top"
                      app:layout_constraintStart_toStartOf="parent"
                      android:text="$1.49 / month"
                      android:textSize="16sp"
                      android:textColor="#708090"
            />
            <ProgressBar android:layout_width="0dp"
                         android:layout_height="5dp"
                         android:id="@+id/progress_bar_end_top"
                         app:layout_constraintStart_toStartOf="parent"
                         app:layout_constraintEnd_toEndOf="parent"
                         app:layout_constraintBottom_toBottomOf="parent"
                         android:progress="100"
                         style="@style/Base.Widget.AppCompat.ProgressBar.Horizontal"
            />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
            android:layout_width="0dp"
            android:onClick="onChangeGuide"
            android:contentDescription="start_bottom"
            android:id="@+id/view_start_bottom"
            app:layout_constraintTop_toBottomOf="@id/guide_h"
            app:layout_constraintEnd_toStartOf="@id/guide_bottom_v"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@android:color/white"
            android:layout_margin="4dp"
            android:layout_height="0dp"
            app:cardCornerRadius="8dp">

        <androidx.constraintlayout.widget.ConstraintLayout
                android:orientation="vertical"
                android:padding="16dp"
                android:layout_width="match_parent"
                android:id="@+id/scene_root_constraint_start_bottom"
                android:background="@android:color/white"
                android:layout_height="match_parent">


            <ImageView android:layout_width="20dp"
                       app:layout_constraintEnd_toEndOf="parent"
                       app:layout_constraintTop_toTopOf="parent"
                       android:layout_height="20dp"
                       android:id="@+id/img_icon_start_bottom"
                       android:src="@mipmap/ic_launcher_round"
            />


            <TextView android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:text="1 GB"
                      android:id="@+id/tv_size_start_bottom"
                      android:textSize="28sp"
                      android:textStyle="bold"
                      app:layout_constraintBottom_toTopOf="@id/tv_start_bottom_price"
                      app:layout_constraintStart_toStartOf="parent"
            />

            <TextView android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:id="@+id/tv_start_bottom_price"
                      android:visibility="gone"
                      app:layout_constraintBottom_toTopOf="@id/progress_bar_start_bottom"
                      app:layout_constraintStart_toStartOf="parent"
                      android:text="$1.49 / month"
                      android:textSize="16sp"
                      android:textColor="#708090"
            />
            <ProgressBar android:layout_width="0dp"
                         android:layout_height="5dp"
                         android:id="@+id/progress_bar_start_bottom"
                         app:layout_constraintStart_toStartOf="parent"
                         app:layout_constraintEnd_toEndOf="parent"
                         app:layout_constraintBottom_toBottomOf="parent"
                         android:progress="100"
                         style="@style/Base.Widget.AppCompat.ProgressBar.Horizontal"
            />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
            android:layout_width="0dp"
            android:onClick="onChangeGuide"
            android:id="@+id/view_end_bottom"
            android:background="@android:color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@id/guide_h"
            app:layout_constraintStart_toEndOf="@id/guide_bottom_v"
            android:contentDescription="end_bottom"
            android:layout_margin="4dp"
            android:layout_height="0dp"
            app:cardCornerRadius="8dp">

        <androidx.constraintlayout.widget.ConstraintLayout
                android:orientation="vertical"
                android:id="@+id/scene_root_constraint_end_bottom"
                android:padding="16dp"
                android:layout_width="match_parent"
                android:background="@android:color/white"
                android:layout_height="match_parent">

            <ImageView android:layout_width="20dp"
                       app:layout_constraintEnd_toEndOf="parent"
                       app:layout_constraintTop_toTopOf="parent"
                       android:layout_height="20dp"
                       android:id="@+id/img_icon_end_bottom"
                       android:src="@mipmap/ic_launcher_round"
            />


            <TextView android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:text="1 GB"
                      android:id="@+id/tv_size_end_bottom"
                      android:textSize="28sp"
                      android:textStyle="bold"
                      app:layout_constraintBottom_toTopOf="@id/tv_end_bottom_price"
                      app:layout_constraintStart_toStartOf="parent"
            />

            <TextView android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:id="@+id/tv_end_bottom_price"
                      android:visibility="gone"
                      app:layout_constraintBottom_toTopOf="@id/progress_bar_end_bottom"
                      app:layout_constraintStart_toStartOf="parent"
                      android:text="$1.49 / month"
                      android:textSize="16sp"
                      android:textColor="#708090"
            />
            <ProgressBar android:layout_width="0dp"
                         android:layout_height="5dp"
                         android:id="@+id/progress_bar_end_bottom"
                         app:layout_constraintStart_toStartOf="parent"
                         app:layout_constraintEnd_toEndOf="parent"
                         app:layout_constraintBottom_toBottomOf="parent"
                         android:progress="100"
                         style="@style/Base.Widget.AppCompat.ProgressBar.Horizontal"
            />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>


</merge>